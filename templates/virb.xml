        <bg>#0A640A80</bg>
        <fg>#00FFFF80</fg>
        <border>#000000C8</border>
        <mark>DEFAULT</mark>
    </colors>
    <fonts>
      <mark fontname="DEFAULT" size="DEFAULT-12" />
    </fonts>
</bearing>

<gpsinfo label="GPS Info" metric="current_position">
