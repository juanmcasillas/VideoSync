<!-- Elevation map -->

<altgraph label="Altitude" metric="position_index, distance, elevation" halign="RIGHT">
    <position x="RIGHT-20" y="BOTTOM-138" />
    <graph width="256" height="128" alpha="40" markradius="1" pointradius="2" xgap="10" ygap="20" />
    <colors>
        <bg>#90909028</bg>
        <fg>#20202048</fg>

        <border>#90909028</border>
        <line>#A0A0A0FF</line>
        <markpoint>#FEFEFEFF</markpoint>

        <start>#909090FF</start>
        <end>#909090FF</end>
        <mark>DEFAULT</mark>
        <label>DEFAULT</label>
    </colors>
    <fonts>
      <mark fontname="DEFAULT" size="DEFAULT-13" />
      <label fontname="DEFAULT" size="DEFAULT-12" />
    </fonts>
</altgraph>

<!-- map -->

<osmmap label="OSM MAP" metric="position_index">
    <position x="LEFT+20" y="TOP+40" />
    <map width="256" height="200" empty="False" alpha="80"/>

    <colors>
        <title>DEFAULT</title>
        <value>DEFAULT</value>
        <start>#00C800FF</start>
        <end>#0000C8FF</end>
        <point>#C80000FF</point>
        <border>#20202048</border>
        <bg>#90909028</bg>
    </colors>
</osmmap>

<bearing label="DEG" metric="bearing">
    <position x="LEFT+20+256-40" y="TOP+45" />
    <compass width="32" height="32" alpha="80" markradius="2" pointradius="3" ygap="15" png="resources/arrow_dark.png"/>
